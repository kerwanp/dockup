{
  "name": "dockup",
  "type": "module",
  "version": "0.0.0-alpha.6",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kerwanp/dockup.git"
  },
  "license": "FSL-1.1-ALv2",
  "bugs": {
    "url": "https://github.com/kerwanp/dockup/issues"
  },
  "homepage": "https://dockup.dev",
  "files": [
    "build",
    "!build/tests",
    "!build/bin/test*"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "quick:test": "node --import=./tsnode.esm.js --enable-source-maps bin/test.ts",
    "test": "c8 yarn quick:test",
    "typecheck": "tsc --noEmit",
    "dev": "tsc --watch"
  },
  "bin": "./build/bin/dockup.js",
  "exports": {
    ".": "./build/modules/index.js",
    "./config": "./build/modules/config.js",
    "./services": "./build/modules/services.js"
  },
  "devDependencies": {
    "@adonisjs/eslint-config": "^2.1.2",
    "@adonisjs/tsconfig": "^1.4.1",
    "@japa/assert": "^4.1.1",
    "@japa/expect": "^3.0.6",
    "@japa/expect-type": "^2.0.3",
    "@japa/file-system": "^2.3.2",
    "@japa/runner": "^4.4.0",
    "@swc/core": "^1.13.3",
    "@types/dockerode": "^3.3.43",
    "@types/node": "^24.2.1",
    "@types/react": "^19",
    "c8": "^10.1.3",
    "eslint": "^9.33.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "packageManager": "yarn@4.9.2",
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@zenobius/ink-mouse": "^1.0.3",
    "c12": "^3.2.0",
    "chalk": "^5.6.0",
    "citty": "^0.1.6",
    "defu": "^6.1.4",
    "dockerode": "^4.0.7",
    "execa": "^9.6.0",
    "hookable": "^5.5.3",
    "ink": "^6.2.2",
    "magicast": "^0.3.5",
    "nypm": "^0.6.1",
    "pathe": "^2.0.3",
    "pkg-types": "^2.3.0",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "zod": "^4.0.17"
  },
  "keywords": [
    "cli",
    "docker",
    "devops",
    "environment"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  }
