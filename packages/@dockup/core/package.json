{
  "name": "@dockup/core",
  "type": "module",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kerwanp/dockup.git"
  },
  "bugs": {
    "url": "https://github.com/kerwanp/dockup/issues"
  },
  "homepage": "https://github.com/kerwanp/dockup",
  "files": [
    "build",
    "!build/tests",
    "!build/bin/schemas*",
    "!build/bin/registry*",
    "!build/bin/test*"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "quick:test": "node --import=./tsnode.esm.js --enable-source-maps bin/test.ts",
    "test": "c8 yarn quick:test",
    "typecheck": "tsc --noEmit",
    "dev": "tsc --watch"
  },
  "bin": "./build/bin/dockup.js",
  "exports": {
    ".": "./build/src/index.js"
  },
  "devDependencies": {
    "@adonisjs/eslint-config": "^2.1.2",
    "@adonisjs/tsconfig": "^1.4.1",
    "@japa/assert": "^4.1.1",
    "@japa/expect": "^3.0.6",
    "@japa/expect-type": "^2.0.3",
    "@japa/file-system": "^2.3.2",
    "@japa/runner": "^4.4.0",
    "@swc/core": "^1.13.3",
    "@types/dockerode": "^3.3.43",
    "@types/node": "^24.2.1",
    "c8": "^10.1.3",
    "eslint": "^9.33.0",
    "execa": "^9.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "packageManager": "yarn@4.9.2",
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "citty": "^0.1.6",
    "dockerode": "^4.0.7",
    "pathe": "^2.0.3"
  },
  "keywords": [
    "cli",
    "ai",
    "cursor",
    "opencode",
    "claude",
    "copilot"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  }
}
